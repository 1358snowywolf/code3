#include <bits/stdc++.h>

using namespace std;

int main() {
	int N, P, K;
	cin >> N >> P >> K;
	map<int, vector<pair<int, int>>> graph;
	vector<pair<int, int>> neighbors;

	for(int i = 0; i < N; i++){
		graph.insert(i, neighbors);
	}

	return 0;
}
