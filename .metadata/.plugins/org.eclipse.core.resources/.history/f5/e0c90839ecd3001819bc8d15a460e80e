#include <bits/stdc++.h>

using namespace std;

int main() {
	vector<int> values = {3, 34, 4, 12, 5, 2};
	int sum = 9;
	vector<vector<bool>> DP (values.size(), vector<int> (sum + 1, false));

	for(int i = 0; i < (int) DP.size(); i++){
		DP[i][0] = true;
	}

	return 0;
}
