#include <bits/stdc++.h>

using namespace std;

int main() {
	int V, N;
	vector<int> coins;
	ifstream fin ("money.in");
	ofstream four ("money.out");

	fin >> V >> N;

	for(int i = 0; i < V; i++){
		int input;
		fin >> input;
		coins.push_back(input);
	}

	vector<int> values (N + 1, 0);
	values[0] = 1;

	for(int i = 0; i < (int) values.size(); i++){
		for(int j = 0; j < (int) coins.size(); j++){
		}
	}

	return 0;
}
