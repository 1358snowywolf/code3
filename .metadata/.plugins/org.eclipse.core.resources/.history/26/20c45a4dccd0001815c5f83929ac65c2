#include <bits/stdc++.h>

using namespace std;

int main() {
	int NR, NC;
	cin >> NR >> NC;
	vector<vector<int>> field;

	for(int i = 0; i < NR; i++){
		vector<int> row;

		for(int j = 0; j < NC; j++){
			int input;
			cin >> input;
			row.push_back(input);
		}

		field.push_back(row);
	}

	return 0;
}
