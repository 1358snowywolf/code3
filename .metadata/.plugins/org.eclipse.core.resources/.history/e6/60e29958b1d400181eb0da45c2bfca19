#include <bits/stdc++.h>

using namespace std;

int main() {
	int N;
	cin >> N;
	vector<int> heights;

	for(int i = 0; i < N; i++){
		int input;
		cin >> input;
		heights.push_back(input);
	}

	return 0;
}
