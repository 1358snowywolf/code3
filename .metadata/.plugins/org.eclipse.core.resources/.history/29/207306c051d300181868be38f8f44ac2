//============================================================================

#include <bits/stdc++.h>

using namespace std;

int main() {
	int value[] = {2, 3, 5};
	int index[] = {0, 0, 0};
	vector<int> dp (1, 1);
	int N;
	cin >> N;

	while((int) dp.size() < N - 1){
		int minimum = INT_MAX;

		for(int i = 0; i < (int) (sizeof(value) / sizeof(int)); i++){
			minimum = min(minimum, dp[index[i]] * value[i]);
		}

		dp.push_back(minimum);

		for(int i = 0; i < (int) (sizeof(index) / sizeof(int)); i++){
			if(dp[index[i]] * value[i] == minimum){
				index[i] = dp.size() - 1;
			}
		}
	}

	cout << dp[N - 1] << endl;


	return 0;
}
