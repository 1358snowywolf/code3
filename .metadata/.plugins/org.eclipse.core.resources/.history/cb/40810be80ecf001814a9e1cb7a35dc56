#include <bits/stdc++.h>

using namespace std;

int findFirst(vector<pair<int, int>> results){
	int first = 0;

	for(int i = 0; i < (int) results.size(); i++){
		if(results[first].first < results[i].first){
			first = i;
		}
	}

	return first;
}

