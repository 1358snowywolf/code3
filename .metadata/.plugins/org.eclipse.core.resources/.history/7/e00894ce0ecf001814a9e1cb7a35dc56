#include <bits/stdc++.h>

using namespace std;

int main() {
	int N, K;
	cin >> N >> K;
	vector<pair<int, int>> results;

	for(int i = 0; i < N; i++){
		int first, second;
		cin >> first >> second;
		results.push_back({first, second});
	}
	return 0;
}
