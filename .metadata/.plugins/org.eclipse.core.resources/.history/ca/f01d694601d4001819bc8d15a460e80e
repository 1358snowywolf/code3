#include <bits/stdc++.h>

using namespace std;

int main() {
	int N;
	cin >> N;
	vector<int> song;
	vector<int> chord;

	for(int i = 0; i < N; i++){
		int input;
		cin >> input;
		song.push_back(input);
	}

	int C;
	cin >> C;

	for(int i = 0; i < C; i++){
		int input;
		cin >> input;
		song.push_back(input);
	}

	return 0;
}
